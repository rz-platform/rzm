{
  "name": "razam-front",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build stylesheets/main.scss javascript/*  --no-source-maps --out-dir ../public/dist --no-cache",
    "watch": "parcel watch stylesheets/main.scss javascript/*",
    "prettier": "prettier --write ./",
    "prettierViews": "prettier --write ../app/views --print-width 50 --parser html --html-whitespace-sensitivity strict"
  },
