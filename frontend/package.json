{
  "name": "rzm-front",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production",
    "watch": "webpack --watch --mode=development",
    "prettier": "prettier --write './{app,stylesheets}/**/*.{js,ts,scss}'",
    "check": "npm run build && npm run check:types && npm run check:prettier && npm run check:lint && npm run check:stylelint",
    "check:types": "tsc -p tsconfig.typecheck.json --noEmit",
    "check:prettier": "prettier --check './{app,stylesheets}/**/*.{js,ts,scss}'",
    "check:lint": "eslint --ext=.ts,.tsx,.js,.jsx app/*",
    "check:stylelint": "stylelint 'stylesheets/**/*.scss'"
  },
  "author": "Eugene Bosiakov",
  "license": "Apache-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.12.1",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.2.1",
    "prettier": "^2.1.2",
    "sass": "^1.22.10",
    "sass-loader": "latest",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "@codemirror/view": "^0.18.4",
    "@codemirror/state": "^0.18.4",
    "@codemirror/matchbrackets": "^0.18",
    "@codemirror/highlight": "^0.18",
    "@codemirror/language": "^0.18",
    "@codemirror/commands": "^0.18",
    "@codemirror/history": "^0.18",
    "@codemirror/legacy-modes": "^0.18",
    "@codemirror/stream-parser": "^0.18"
  }
}
