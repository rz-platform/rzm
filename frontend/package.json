  "license": "Apache 2.0",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "prettier": "^2.1.2",
    "sass": "^1.26.10",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "codemirror": "^5.50.2"
  }
}
